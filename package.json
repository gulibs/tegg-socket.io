{
  "name": "@gulibs/tegg-socket.io",
  "version": "0.0.9",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "description": "",
  "eggPlugin": {
    "name": "teggSocketIO",
    "exports": {
      "import": "./dist/esm",
      "require": "./dist/commonjs",
      "typescript": "./src"
    }
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-tegg-socket.io",
    "teggSocketIO"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gulibs/tegg-socket.io.git"
  },
  "bugs": {
    "url": "https://github.com/gulibs/tegg-socket.io/issues"
  },
  "homepage": "https://github.com/gulibs/tegg-socket.io/blob/main/README.md",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">= 18.19.0"
  },
  "dependencies": {
    "@eggjs/core": "^6.2.13",
    "debug": "^4.4.3",
    "delegates": "^1.0.0",
    "koa-compose": "^4.1.0",
    "socket.io": "^4.8.1",
    "socket.io-redis": "^6.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@eggjs/bin": "7",
    "@eggjs/mock": "6",
    "@eggjs/tsconfig": "1",
    "@types/debug": "^4.1.12",
    "@types/delegates": "^1.0.3",
    "@types/koa-compose": "^3.2.9",
    "@types/mocha": "10",
    "@types/node": "22",
    "@types/socket.io": "^3.0.2",
    "egg": "4",
    "egg-core": "^5.5.1",
    "egg-ts-helper": "^1.35.2",
    "eslint": "8",
    "eslint-config-egg": "14",
    "rimraf": "6",
    "socket.io-client": "^4.8.1",
    "tshy": "3",
    "tshy-after": "1",
    "typescript": "5"
  },
  "scripts": {
    "lint": "eslint --cache src test --ext .ts",
    "pretest": "npm run clean && npm run lint -- --fix",
    "test": "egg-bin test",
    "preci": "npm run clean &&  npm run lint",
    "ci": "egg-bin cov",
    "postci": "npm run prepublishOnly && npm run clean",
    "clean": "rimraf dist",
    "prepublishOnly": "tshy && tshy-after && attw --pack"
  },
  "type": "module",
  "tshy": {
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "README.zh_CN.md",
    "tshelper.json"
  ],
  "types": "./dist/commonjs/index.d.ts",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js"
}