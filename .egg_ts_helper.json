{
    "watchDirs": {
        "io/middleware": {
            "path": "app/io/middleware",
            "interface": "CustomMiddleware",
            "generator": "function",
            "enabled": true,
            "caseStyle": "camel",
            "interfaceHandle": "val => val"
        },
        "io/controller": {
            "path": "app/io/controller",
            "interface": "CustomController",
            "generator": "class",
            "enabled": true,
            "caseStyle": "camel",
            "interfaceHandle": "val => val"
        }
    }
}